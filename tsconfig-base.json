{
  "compilerOptions": {
      "declaration": true,
      "target": "es6",
      "module": "commonjs",
      "strict": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noImplicitReturns": true,
      "noFallthroughCasesInSwitch": true,
      "composite": true,
      "esModuleInterop": true,
      "incremental": true
  }
}